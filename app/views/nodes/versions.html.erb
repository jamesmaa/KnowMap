<%= render :partial => "application/flash", :locals => { :flash => flash } %>
<h1>Versions for: <%= @node.title %></h1>
<% if @versions.count == 0 %>
  <i>No previous versions exist</i>
<% else %>
  <% @versions.each { |version| %>
    <%= link_to version.reify, node_version_path(@node, :version => version.id) %><br />
  <% } %>
<% end %>
