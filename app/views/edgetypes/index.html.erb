<h1>Listing edgetypes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Desc</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @edgetypes.each do |edgetype| %>
  <tr>
    <td><%= edgetype.name %></td>
    <td><%= edgetype.desc %></td>
    <td><%= link_to 'Show', edgetype %></td>
    <td><%= link_to 'Edit', edit_edgetype_path(edgetype) %></td>
    <td><%= link_to 'Destroy', edgetype, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

